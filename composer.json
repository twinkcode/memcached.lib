{
  "name": "twinkcode/memcached-library",
  "description": "A client library for Memcached, implemented in PHP.",
  "authors": [
    {
      "name": "twinkcode",
      "email": "twinkcode@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-sockets": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1.3"
  },
  "autoload": {
    "psr-4": {
      "MemcachedClient\\": "src/"
    }
  },

  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
